    "@testing-library/angular": "^17.3.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@types/jasmine": "~5.1.0",
    "cypress": "^13.16.0",
    "jasmine-core": "~5.2.0",
