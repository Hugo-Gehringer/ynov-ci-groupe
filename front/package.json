    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.7",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.13",
    "angular-cli-ghpages": "^2.0.1",
    "dotenv": "^16.4.7",
    "hugo-gehringer-cicd": "^0.0.1",
    "jest": "^29.7.0",
    "mysql": "^2.18.1",
