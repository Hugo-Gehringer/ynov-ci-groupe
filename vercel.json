{
  "version": 2,
  "builds": [
    {
      "src": "back/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "back/**"
        ],
        "ignore": [
          "front/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "back/server.js"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": false
    }
  }
}